#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/bash" ]; then
  exec "$basedir/bash"  "$basedir/../bash.origin/bin/bash.origin.script" "$@"
else 
  exec bash  "$basedir/../bash.origin/bin/bash.origin.script" "$@"
fi
